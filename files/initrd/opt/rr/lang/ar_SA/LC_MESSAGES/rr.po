msgstr " * \\Z4DiskIdxMap=??\\Zn\\n    تعيين ترتيب أرقام الأقراص (لا ينطبق على الموديلات DT).\\n"

#: menu.sh:887
msgid " * \\Z4ahci_remap=4>5:5>8:12>16\\Zn\\n    Remap data port sequence(Not apply to DT models).\\n"
