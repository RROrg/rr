#: menu.sh:879
msgid "Commonly used cmdlines:\\n"
msgstr "سطر الأوامر الشائعة:\\n"

#: menu.sh:880
msgid " * \\Z4SpectreAll_on=\\Zn\\n    Enable Spectre and Meltdown protection to mitigate the threat of speculative execution vulnerability.\\n"
msgstr " * \\Z4SpectreAll_on=\\Zn\\n    قم بتمكين حماية Spectre وMeltdown للتخفيف من خطر ثغرة التنفيذ\\n"

#: menu.sh:881
msgid " * \\Z4disable_mtrr_trim=\\Zn\\n    disables kernel trim any uncacheable memory out.\\n"
msgstr " * \\Z4disable_mtrr_trim=\\Zn\\n    تعطيل محرك التفريغ من الذاكرة غير التخزينية.\\n"

#: menu.sh:882
msgid " * \\Z4intel_idle.max_cstate=1\\Zn\\n    Set the maximum C-state depth allowed by the intel_idle driver.\\n"
msgstr " * \\Z4intel_idle.max_cstate=1\\Zn\\n    تعيين عمق الحالة C الأقصى المسموح به من قبل محرك intel_idle.\\n"

#: menu.sh:883
msgid " * \\Z4pcie_port_pm=off\\Zn\\n    Turn off the power management of the PCIe port.\\n"
msgstr " * \\Z4pcie_port_pm=off\\Zn\\n    تعطيل إدارة الطاقة من منفذ PCIe.\\n"

#: menu.sh:884
msgid " * \\Z4libata.force=noncq\\Zn\\n    Disable NCQ for all SATA ports.\\n"
msgstr " * \\Z4libata.force=noncq\\Zn\\n    تعطيل NCQ لجميع منافذ SATA.\\n"

#: menu.sh:885
msgid " * \\Z4SataPortMap=??\\Zn\\n    Sata Port Map(Not apply to DT models).\\n"
msgstr " * \\Z4SataPortMap=??\\Zn\\n    تعيين ترتيب منفصلات SATA (لا ينطبق على الموديلات DT).\\n"

#: menu.sh:886
